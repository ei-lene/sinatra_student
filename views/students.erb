<html>
  <head>
    <meta charset="utf-8" />
    <title></title>
    <link href="style.css" rel="stylesheet" />
  </head>
  <body>

    <div class="header"><h1>Flatiron Students Winter 2013</h1></div>

    <div class="index">     
       <% i=0 %>
       <% @students.each do |student| %>
          <%= i += 1 %>
          <a href="/<%= student.slug %>"><%= student.name %></a><br><br>
       <% end %>
    </div>

  </body>
</html>
